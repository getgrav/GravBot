{
  "branchPrefix": "dep/",
  "devDependencies": {
    "branchPrefix": "dev-dep/"
  },
  "enabled": true,
  "ignorePaths": [
    "**/node_modules/**",
    "**/bower_components/**",
    "**/__tests__/**",
    "**/test/**",
    "**/tests/**"
  ],
  "labels": [
    "dependencies"
  ],
  "optionalDependencies": {
    "branchPrefix": "opt-dep/"
  },
  "patch": {
    "automerge": true,
    "automergeType": "branch-push",
    "commitBody": "[skip ci]"
  },
  "pinVersions": false,
  "rebaseStalePrs": false,
  "semanticCommitScope": null,
  "unpublishSafe": true
}
